{
  "name": "@nezu/cli",
  "version": "0.0.1",
  "description": "CLI for creating TypeScript/JavaScript project",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "bin": {
    "nezu": "lib/index.js"
  },
  "files": [
    "lib",
    "templates"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./lib",
    "compile": "tsc -p tsconfig.json",
    "prepublish": "yarn run build"
  },
  "author": "al002",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^6.1.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.11.8",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
  }
}
